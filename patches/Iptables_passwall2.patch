 define Package/$(PKG_NAME)/config
@@ -40,35 +34,10 @@ menu "Configuration"
 config PACKAGE_$(PKG_NAME)_INCLUDE_IPv6_Nat
 	depends on PACKAGE_ip6tables
 	bool "Include IPv6 Nat"
